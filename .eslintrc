{
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "es6": true
  },
  "rules": {
    "prettier/prettier": "error"
  },
  "overrides": [{
    "files": "**/*.test.js",
    "env": {
      "jest": true
    }
  }]
}
